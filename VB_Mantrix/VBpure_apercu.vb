        Public Sub Form_Load(ByVal sender As Object, ByVal e As System.EventArgs)
            'Cette fonction se déclenche à l'ouverture de la fenêtre.
            '
            'This function is launched during opening.
            gest_obtenir_dossier = New System.Collections.Generic.List(Of Object)(System.IO.Directory.GetDirectories("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\", "*.*", System.IO.SearchOption.TopDirectoryOnly))
            gest_startboucle = 0
            Do While (gest_startboucle < gest_obtenir_dossier.Count)
                Me.listBox1.Items.Add("" & gest_obtenir_dossier(gest_startboucle) & "")
                gest_startboucle = (gest_startboucle + 1)
            Loop
            gest_determin_images = _computer.FileSystem.DirectoryExists("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\images")
            gest_determin_divers = _computer.FileSystem.DirectoryExists("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\divers")
            gest_determin_videos = _computer.FileSystem.DirectoryExists("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\vidéos")
            gest_determin_musiques = _computer.FileSystem.DirectoryExists("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\musiques")
            If (gest_determin_divers = True) Then
                Me.pictureBox1.Enabled = True
                Me.pictureBox1.Image = My.Resources.dossierimg_divers
            End If
            If (gest_determin_images = True) Then
                Me.pictureBox2.Enabled = True
                Me.pictureBox2.Image = My.Resources.dossierimg_images
            End If
            If (gest_determin_musiques = True) Then
                Me.pictureBox3.Enabled = True
                Me.pictureBox3.Image = My.Resources.dossierimg_musiques
            End If
            If (gest_determin_videos = True) Then
                Me.pictureBox4.Enabled = True
                Me.pictureBox4.Image = My.Resources.dossierimg_videos
            End If
            Me.openFileDialog2.InitialDirectory = "" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\divers"
            Me.openFileDialog3.InitialDirectory = "" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\images"
            Me.openFileDialog4.InitialDirectory = "" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\musiques"
            Me.openFileDialog5.InitialDirectory = "" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & username_connexion & "\vidéos"
        End Sub
        
