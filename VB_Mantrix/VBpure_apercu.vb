    Public Class gestionnaire_users
        
        'Fonction Form_Load
        Public Sub Form_Load(ByVal sender As Object, ByVal e As System.EventArgs)
            'Cette fonction se déclenche à l'ouverture de la fenêtre.
            '
            'This function is launched during opening.
            determin_hostnamefile = _computer.FileSystem.FileExists("" & System.Windows.Forms.Application.StartupPath & "\System\Core\hostname.conf")
            If (determin_hostnamefile = True) Then
                lire_hostname_attrib = _computer.FileSystem.ReadAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Core\hostname.conf", System.Text.Encoding.UTF8)
                Me.textBox8.Text = "" & lire_hostname_attrib & ""
            Else
                aleatoire_code = VelerSoftware_GeneralPlugin.RandomNumber(0, 99999999, true)
                Me.textBox8.Text = "MANTSERV-F" & aleatoire_code & "x" & aleatoire_code & "vGt" & aleatoire_code & "h" & aleatoire_code & "k"
                _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Core\hostname.conf", "MANTSERV-F" & aleatoire_code & "x" & aleatoire_code & "vGt" & aleatoire_code & "h" & aleatoire_code & "k", False, System.Text.Encoding.UTF8)
            End If
        End Sub
        
        'Fonction create_user_password_Users_Click
        Public Sub create_user_password_Users_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            new_user_Users = "" & Me.textBox1.Text & ""
            new_password_un_Users = "" & Me.textBox2.Text & ""
            new_password_deux_Users = "" & Me.textBox3.Text & ""
            determin_newuser_exist = _computer.FileSystem.FileExists("" & System.Windows.Forms.Application.StartupPath & "\System\Users\" & new_user_Users & ".usrx")
            If (determin_newuser_exist = True) Then
                System.Windows.Forms.MessageBox.Show("Désolé un compte existe déjà avec le nom suivant : " & new_user_Users & "" & System.Environment.NewLine & "Vous ne pouvez donc pas le remplacer." & System.Environment.NewLine & "Conseil : Supprimer l'ancien compte s'il n'est plus utilisé et recréer le. S'il s'agit d'un problème de mot de passe un onglet spécifique existe.", "Création nouveau compte", System.Windows.Forms.MessageBoxButtons.OK, System.Windows.Forms.MessageBoxIcon.Warning)
            Else
                If (new_password_un_Users = "" & new_password_deux_Users & "") Then
                    _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Users\" & new_user_Users & ".usrx", "" & new_password_un_Users & "", False, System.Text.Encoding.UTF8)
                    Verif_dossier_persousers = _computer.FileSystem.DirectoryExists("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "")
                    If (Verif_dossier_persousers = True) Then
                    Else
                        _computer.FileSystem.CreateDirectory("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "")
                        _computer.FileSystem.CreateDirectory("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\images")
                        _computer.FileSystem.CreateDirectory("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\musiques")
                        _computer.FileSystem.CreateDirectory("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\vidéos")
                        _computer.FileSystem.CreateDirectory("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\divers")
                        _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\Vdesktop.var", "SCREEN_SAVE=12", False, System.Text.Encoding.UTF8)
                        _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\Vinfinitybar.var", "COLOR_BAR=02", False, System.Text.Encoding.UTF8)
                        _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\icobur.cla", "", False, System.Text.Encoding.UTF8)
                        _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Home\" & new_user_Users & "\iconet.cla", "", False, System.Text.Encoding.UTF8)
