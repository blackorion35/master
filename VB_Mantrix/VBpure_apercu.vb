        'Fonction Form_Load
        Public Sub Form_Load(ByVal sender As Object, ByVal e As System.EventArgs)
            'Cette fonction se déclenche à l'ouverture de la fenêtre.
            '
            'This function is launched during opening.
            determin_licencekey = _computer.FileSystem.FileExists("" & System.Windows.Forms.Application.StartupPath & "\System\Dev\keyboxos.vksx")
            If (determin_licencekey = False) Then
                _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Dev\keyboxos.vksx", "", False, System.Text.Encoding.UTF8)
            End If
            lecture_licencekey = _computer.FileSystem.ReadAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Dev\keyboxos.vksx", System.Text.Encoding.UTF8)
            If (lecture_licencekey = "ERSMX-112X3-QR7SM-TR39Q-TH781") Then
                Me.textBox3.Text = "Votre license d'utilisation de Mantrix est valide."
            Else
                Me.textBox3.Text = "Votre license d'utilisation de Mantrix est invalide !"
            End If
        End Sub
        
        'Fonction connexion_users_Click
        Public Sub connexion_users_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            username_connexion = "" & Me.textBox1.Text & ""
            userpassword_connexion = "" & Me.textBox2.Text & ""
            determin_userfile = _computer.FileSystem.FileExists("" & System.Windows.Forms.Application.StartupPath & "\System\Users\" & username_connexion & ".usrx")
            If (determin_userfile = True) Then
                lecture_password = _computer.FileSystem.ReadAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Users\" & username_connexion & ".usrx", System.Text.Encoding.UTF8)
                If (lecture_password = "" & userpassword_connexion & "") Then
                    Variables.Mantrix_Session_Persousers.ShowDialog
                Else
                    System.Windows.Forms.MessageBox.Show("Votre compte semble disponible mais votre mot de passe n'est pas correct." & System.Environment.NewLine & "Réessayer d'entrer à nouveau votre mot de passe.", "Erreur de mot de passe", System.Windows.Forms.MessageBoxButtons.OK, System.Windows.Forms.MessageBoxIcon.Warning)
                    Me.textBox2.Text = ""
                End If
            Else
                System.Windows.Forms.MessageBox.Show("Votre nom d'utilisateur n'existe pas. Vérifier votre nom d'utilisateur pour continuer.", "Compte introuvable", System.Windows.Forms.MessageBoxButtons.OK, System.Windows.Forms.MessageBoxIcon.Warning)
            End If
            Me.textBox2.Text = ""
        End Sub
        
        'Fonction shutdown_mantrixsystemos_Click
        Public Sub shutdown_mantrixsystemos_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            _computer.Audio.Play("" & System.Windows.Forms.Application.StartupPath & "\System\Sons\close.wav", Microsoft.VisualBasic.AudioPlayMode.WaitToComplete)
            System.Windows.Forms.Application.Exit
        End Sub
        
        'Fonction connexion_MouseDown
        Public Sub connexion_MouseDown(ByVal sender As Object, ByVal e As System.Windows.Forms.MouseEventArgs)
 If e.Button = System.Windows.Forms.MouseButtons.Left Then
            IsFormBeingDragged = True
            MouseDownX = e.X
            MouseDownY = e.Y
        End If
        End Sub
        
        'Fonction connexion_MouseMove
        Public Sub connexion_MouseMove(ByVal sender As Object, ByVal e As System.Windows.Forms.MouseEventArgs)
  If IsFormBeingDragged Then
            Dim temp As Drawing.Point = New Drawing.Point()
            temp.X = Me.Location.X + (e.X - MouseDownX)
            temp.Y = Me.Location.Y + (e.Y - MouseDownY)
            Me.Location = temp
            temp = Nothing
        End If
        End Sub
        
