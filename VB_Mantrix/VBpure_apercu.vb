        'Fonction ajout_ports_entrants_Click
        Public Sub ajout_ports_entrants_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            _computer.FileSystem.WriteAllText("" & System.Windows.Forms.Application.StartupPath & "\System\Dev\firewall.kim", "ENT=0x741D55519CBBA8311(0;A;G;H)// Entrants ports" & System.Environment.NewLine & "", True, System.Text.Encoding.UTF8)
